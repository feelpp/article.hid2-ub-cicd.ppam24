	language = {en},
	urldate = {2022-08-24},
	booktitle = {Fluid {Structure} {Interaction} {II}},
	publisher = {Springer Berlin Heidelberg},
	author = {Turek, S. and Hron, J. and M√°dl√≠k, M. and Razzaq, M. and Wobker, H. and Acker, J. F.},
